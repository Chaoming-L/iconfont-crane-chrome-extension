{
  "name": "iconfont-helper",
  "description": "支持iconfont一键添加购物车、一键购物车下载的等功能。http://github.com/dawangraoming",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "iconfont helper",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.iconfont.cn/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "lib/jszip.min.js",
        "script.js"
      ]
    }
  ],
  "manifest_version": 2
}

{
  "name": "iconfont-crane",
  "description": "支持iconfont下载svg等功能。https://github.com/chaoming56",
  "version": "0.1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "iconfont crane",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.iconfont.cn/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "lib/jszip.min.js",
        "script.js"
      ]
    }
  ],
  "manifest_version": 2
}
